{
  "consul": "127.0.0.1:8500",
  "services": [
    {
      "name": "consul",
      "port": 8500,
      "exec": ["/bin/consul", "agent", "-server",
               "-bootstrap-expect", "3",
               "-config-dir=/etc/consul",
               "-ui-dir", "/ui"],
      "health": ["/usr/local/bin/consul-manage", "health"],
      "preStart": ["/usr/local/bin/consul-manage", "preStart"],
      "preStop": ["consul", "leave"],
      "poll": 10,
      "ttl": 25
    }
  ]
}
